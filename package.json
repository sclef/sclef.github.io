{
  "name": "sclef.github.io",
  "version": "1.0.0",
  "scripts": {
    "build-babel": "babel  src -d lib",
    "watch-babel": "babel src --watch -d lib",
    "build-browsify": "browserify lib/Main.js -o bundle.js",
    "watch-browsify": "watchify lib/Main.js -o bundle.js",
    "start-server": "http-server",
    "build": "npm run build-babel && npm run build-browsify",
    "watch": "npm run watch-babel & npm run watch-browsify",
    "start": "npm run start-server & npm run watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "es6-promise-promise": "^1.0.0",
    "uniq": "^1.0.1",
    "url-polyfill": "^1.1.0",
    "whatwg-fetch": "^3.0.0"
  }
}
