{
  "name": "sclef.github.io",
  "version": "1.0.0",
  "scripts": {
    "build-babel": "babel  src/js -d public/js",
    "watch-babel": "babel src/js --watch -d public/js",
    "build-browsify": "browserify public/js/Main.js -o public/js/bundle.js",
    "watch-browsify": "watchify public/js/Main.js -o public/js/bundle.js",
    "start-server": "http-server ./",
    "build": "npm run build-babel && npm run build-browsify",
    "watch": "npm run watch-babel & npm run watch-browsify",
    "start": "npm run start-server & npm run watch",
    "build-webpack": "webpack --mode development",
    "start-dev-server": "webpack-dev-server --mode development",
    "build-webpack-prod": "webpack --mode production",
    "start-dev-server-prod": "webpack-dev-server --mode production"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "es6-promise-promise": "^1.0.0",
    "uniq": "^1.0.1",
    "url-polyfill": "^1.1.0",
    "whatwg-fetch": "^3.0.0"
  }
}